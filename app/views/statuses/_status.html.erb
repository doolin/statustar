<tr>
  <td class="status">
    <span class="content"><%= status.state %></span>
    <span class="timestamp">
      Posted <%= time_ago_in_words(status.created_at) %> ago.
    </span>
  </td>
  <% if current_user?(status.user) %>
  <td>
    <%= link_to "delete", status, :method => :delete,
                                  :confirm => "You sure?",
                                  :title => status.state %>
  </td>
  <% end %>
</tr>